                          .carousel-caption
                            h3 Coders.Tokyo Da Nang
                            p Join with us
                        .carousel-item
                          img.d-block.w-100(src='/images/coderstokyocantho.jpg?auto=yes&bg=555&fg=333&text=Third slide', alt='Third slide', style='width: 1000px; height: 500px')
                          .carousel-caption
                            h3 Coders.Tokyo Can Tho
                            p Join with us
                      a.carousel-control-prev(href='#secondCarousel', role='button', data-slide='prev')
                        span.carousel-control-prev-icon(aria-hidden='true')
